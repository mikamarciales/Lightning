int startX = 0;
int startY = 150;
int endX = 0;
int endY = 150;

void setup()
{
	size(300,300);
	background(0);
	strokeWeight(5);
}
void draw()
{
	stroke((Math.random()*256), (Math.random()*256), (Math.random()*256);
	while (endX < 300)
	{
		endX = startX + (int)(Math.random()*10);
		endY = startY + (int)(Math.random()*19)-9
	}

}
